<CreateOrder xmlns="http://Fastems.Ui">
    <requestor xmlns:d4p1="http://schemas.datacontract.org/2004/07/Fastems.Mms.Common.Dto" xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
        <d4p1:ApplicationName>DataManager</d4p1:ApplicationName>
        <d4p1:HostName>cnc405-pc</d4p1:HostName>
        <d4p1:UserName>mmsuser</d4p1:UserName>
    </requestor>
    <request xmlns:d4p1="http://schemas.datacontract.org/2004/07/Fastems.Mms.Common.Dto.Orders" xmlns:i="http://www.w3.org/2001/XMLSchema-instance">
        <d4p1:Description>{description}</d4p1:Description>
        <d4p1:DueDate xmlns:d5p1="http://schemas.datacontract.org/2004/07/Fastems.Mms.Common.Dto">
            <d5p1:Utc>{due_date}</d5p1:Utc>
        </d4p1:DueDate>
        <d4p1:EarliestStart xmlns:d5p1="http://schemas.datacontract.org/2004/07/Fastems.Mms.Common.Dto">
            <d5p1:Utc>{start_date}</d5p1:Utc>
        </d4p1:EarliestStart>
        <d4p1:ItemBaseDataId xmlns:d5p1="http://schemas.datacontract.org/2004/07/Fastems.Mms.Common.Dto.BaseData">
            <d5p1:Id>{gid}</d5p1:Id>
            <d5p1:Name>{part_number}</d5p1:Name>
        </d4p1:ItemBaseDataId>
        <d4p1:Number>{order_number}</d4p1:Number>
        <d4p1:Quantity>{quantity}</d4p1:Quantity>
        <d4p1:Status>{status}</d4p1:Status>
    </request>
</CreateOrder>
